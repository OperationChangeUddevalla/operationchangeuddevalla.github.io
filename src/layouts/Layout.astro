	<head>
		<meta charset="UTF-8" />
		<meta name="viewport" content="width=device-width" />

		<link rel="icon" type="image/svg+xml" href="assets/operation.svg" />

		<title>{title}</title>
	</head>
	<body>
		<slot />
	</body>
</html>
